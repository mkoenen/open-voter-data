<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns       = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "tv.loudfeed.govscore-global"
        versionCode = "13"
        version     = "1.2.2">

    <!-- versionCode is optional and Android only -->
    

    <name>Govscore Global</name>

    <description>
        Governance Assessment Tool.
    </description>

    <author href="http://loudfeed.tv" email="margaretekoenen@gmail.com">
        Margarete Koenen
    </author>


    <icon src="res/icons/ios/icon-60@3x.png" gap:platform="ios" width="180" height="180" />
    <icon src="res/icons/ios/icon-60.png" gap:platform="ios" width="60" height="60" />
    <icon src="res/icons/ios/icon-60@2x.png" gap:platform="ios" width="120" height="120" />
    <icon src="res/icons/ios/icon-76.png" gap:platform="ios" width="76" height="76" />
    <icon src="res/icons/ios/icon-76@2x.png" gap:platform="ios" width="152" height="152" />
    <icon src="res/icons/ios/icon-small.png" gap:platform="ios" width="29" height="29" />
    <icon src="res/icons/ios/icon-small@2x.png" gap:platform="ios" width="58" height="58" />
    <icon src="res/icons/ios/icon-40.png" gap:platform="ios" width="40" height="40" />
    <icon src="res/icons/ios/icon-40@2x.png" gap:platform="ios" width="80" height="80" />
    <icon src="res/icons/ios/icon.png" gap:platform="ios" width="57" height="57" />
    <icon src="res/icons/ios/icon@2x.png" gap:platform="ios" width="114" height="114" />
    <icon src="res/icons/ios/icon-72.png" gap:platform="ios" width="72" height="72" />
    <icon src="res/icons/ios/icon-72@2x.png" gap:platform="ios" width="144" height="144" />
    <icon src="res/icons/ios/icon-50.png" gap:platform="ios" width="50" height="50" />
    <icon src="res/icons/ios/icon-50@2x.png" gap:platform="ios" width="100" height="100" />


    <gap:splash src="res/splash/ios/Default~iphone.png" gap:platform="ios" width="320" height="480"/>
    <gap:splash src="res/splash/ios/Default@2x~iphone.png" gap:platform="ios" width="640" height="960"/>
    <gap:splash src="res/splash/ios/Default-Portrait~ipad.png" gap:platform="ios" width="768" height="1024"/>
    <gap:splash src="res/splash/ios/Default-Portrait@2x~ipad.png" gap:platform="ios" width="1536" height="2048"/>
    <gap:splash src="res/splash/ios/Default-Landscape~ipad.png" gap:platform="ios" width="1024" height="768"/>
    <gap:splash src="res/splash/ios/Default-Landscape@2x~ipad.png" gap:platform="ios" width="2048" height="1536"/>
    <gap:splash src="res/splash/ios/Default-568h@2x~iphone.png" gap:platform="ios" width="640" height="1136"/>

    <icon src="icon.png" />
    <icon src="res/icons/android/drawable-ldpi/icon.png" gap:platform="android" gap:qualifier="ldpi" />
    <icon src="res/icons/android/drawable-mdpi/icon.png" gap:platform="android" gap:qualifier="mdpi" />
    <icon src="res/icons/android/drawable-hdpi/icon.png" gap:platform="android" gap:qualifier="hdpi" />
    <icon src="res/icons/android/drawable-xhdpi/icon.png" gap:platform="android" gap:qualifier="xhdpi" />
    <icon src="res/icons/android/drawable-xxhdpi/icon.png" gap:platform="android" gap:qualifier="xxhdpi" />
    <icon src="res/icons/android/drawable-xxxhdpi/icon.png" gap:platform="android" gap:qualifier="xxxhdpi" />
    
    <gap:splash src="splash.png" />
    <gap:splash src="res/splash/android/ldpi.png"  gap:platform="android" gap:qualifier="ldpi" />
    <gap:splash src="res/splash/android/mdpi.png"  gap:platform="android" gap:qualifier="mdpi" />
    <gap:splash src="res/splash/android/hdpi.png"  gap:platform="android" gap:qualifier="hdpi" />
    <gap:splash src="res/spalsh/android/xhdpi.png" gap:platform="android" gap:qualifier="xhdpi" />
    <gap:splash src="res/spalsh/android/xxhdpi.png" gap:platform="android" gap:qualifier="xxhdpi" />
    <gap:splash src="res/splash/android/portrait-xxhdpi.png" gap:platform="android" gap:qualifier="port-xxhdpi" />
    <gap:splash src="res/splash/android/landscape-xxhdpi.png" gap:platform="android" gap:qualifier="land-xxhdpi" />

    <preference name="phonegap-version" value="cli-5.2.0" />
    <preference name="EnableViewportScale" value="true"/>
    <preference name="BackupWebStorage" value="local"/>
    <preference name="KeyboardDisplayRequiresUserAction" value="false"/>
    <preference name="Orientation" value="default" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="SplashScreenDelay" value="4000"/>
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />
    <preference name="SplashScreen" value="splash" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <!-- <preference name="FadeSplashScreen" value="false"/>-->
    <preference name="ShowSplashScreen" value="true" />

    
    <feature name="http://api.phonegap.com/1.0/network"/>
    <feature name="http://api.phonegap.com/1.0/notification"/>
    <feature name="http://api.phonegap.com/1.0/device" />

    
   
    <gap:plugin name="com.pushwoosh.plugins.pushwoosh" version="3.4.2"/>
    <gap:plugin name="org.apache.cordova.network-information" version="0.2.12" />
    <gap:plugin name="org.apache.cordova.dialogs" />
    <gap:plugin name="org.apache.cordova.globalization" version="0.3.1" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" />
    <gap:plugin name="org.apache.cordova.device" version="0.2.12" />
    <gap:plugin name="cordova-plugin-splashscreen" version="2.1.0" source="npm" />
    
    <access origin="*.pushwoosh.com"/> <!-- allow pushwoosh service -->
    <access origin="http://mshlmg.wpengine.com" />


</widget>
